{
  "rules": [
    {
      "rule-type": "selection",
      "rule-id": "1",
      "rule-name": "include-integration-logs",
      "object-locator": {
        "schema-name": "dbo",
        "table-name": "integrationlogs"
      },
      "rule-action": "include"
    },
    {
      "rule-type": "object-mapping",
      "rule-id": "2",
      "rule-name": "map-to-dynamodb",
      "rule-action": "map-record-to-record",
      "rule-target": "table",
      "object-locator": {
        "schema-name": "dbo",
        "table-name": "integrationlogs"
      },
      "target-table-name": "LoanApp-IntegrationLogs-dev",
      "mapping-parameters": {
        "partition-key-name": "logid",
        "sort-key-name": "logtimestamp",
        "attribute-mappings": [
          {
            "target-attribute-name": "logid",
            "attribute-type": "scalar",
            "attribute-sub-type": "number",
            "value": "${logid}"
          },
          {
            "target-attribute-name": "logtimestamp",
            "attribute-type": "scalar",
            "attribute-sub-type": "string",
            "value": "${logtimestamp}"
          }
        ]
      }
    }
  ]
}